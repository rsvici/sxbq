.gopayContent {
    background-color: #f5f5f5;
    min-height: 100vh;
}

.gopayContent .list {
    margin-bottom: 2vw;
}

.gopayContent .fffirst {
    width: 100%;
    margin-top: 2vw;
    background-color: #fff;
    position: relative;
}

.fffirst image {
    width: 750rpx;
    height: 165rpx;
}

.gopayContent .fffirst view {
    font-family: PingFangSC-Regular;
    font-size: 28rpx;
    color: #000;
    height: 7vw;
    line-height: 10vw;
}

.gopayContent .fffirst .left {
    float: left;
    text-align: left;
    width: 50%;
}

.gopayContent .fffirst .right {
    float: right;
    text-align: right;
    width: 50%;
}

.gopayContent .fffirst .second {
    width: 86vw;
    margin: 0 auto;
    font-size: 28rpx;
    height: 10vw;
    line-height: 5vw;
    padding-top: 4vw;
}

.gopayContent .fffirst .ffirst {
    padding: 0 7vw;
}


/* 商品列表 */

.gopayContent .second {
    background-color: #fff;
    height: 240rpx;
}

.gopayContent .second .imgages {
    float: left;
    width: 200rpx;
    height: 200rpx;
}

.gopayContent .second .imgages .navimg {
    width: 180rpx;
    height: 180rpx;
    margin: 30rpx 20rpx;
    vertical-align: top;
}

.gopayContent .second .info {
    float: right;
    width: 500rpx;
    position: relative;
    height: 200rpx;
}

.gopayContent .second .info>view {
    display: block;
    font-size: 12px;
}

.gopayContent .second .info .title {
    width: 100%;
    font-size: 30rpx;
    color: #454545;
    overflow: hidden;
    text-overflow: ellipsis;
    display: box;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    margin-top: 10px;
    line-height: 50rpx;
}

.gopayContent .second .info .specify {
    color: #999;
    margin-top: 10px;
    font-size: 28rpx;
    line-height: 40rpx;
}

.gopayContent .second .info .charge {
    color: #aa821e;
    font-size: 28rpx;
    padding: 5px 0 0 0;
    line-height: 50rpx;
}

.gopayContent .second .info .charge>text {
    display: inline;
}

.gopayContent .second .info .buy-num {
    position: absolute;
    bottom: 10rpx;
    right: 10rpx;
    background: #fff;
    border-radius: 5px;
    color: #4c4c4c;
}

.gopayContent .second .info .buy-num .goodsNum {
    display: inline-block;
    width: 40px;
    height: 30px;
    text-align: center;
    line-height: 30px;
    font-size: 16px;
}


/* 第三层价格 */

.gopayContent .third {
    background-color: #fff;
    height: 15vh;
}

.gopayContent .third .ul .li {
    height: 5vh;
    padding: 0 7vw;
}

.gopayContent .third .ul .li view {
    width: 40%;
    line-height: 5vh;
    font-family: PingFangSC-Regular;
    font-size: 28rpx;
}

.gopayContent .third .ul .li .left {
    float: left;
    text-align: left;
}

.gopayContent .third .ul .li2 .left text {
    height: 28rpx;
    box-sizing: border-box;
    border: 1px solid #967800;
    color: #967800;
    font-size: 25rpx;
    line-height: 28rpx;
    padding: 0 15rpx;
    margin-left: 10rpx;
}

.gopayContent .third .ul .li .right {
    float: right;
    text-align: right;
    font-size: 32rpx;
    font-family: Arial;
    font-weight: 700;
}

.gopayContent .third .ul .li .right text {
    font-size: 28rpx;
}

.gopayContent .third .ul .li .right text.span1 {
    font-family: PingFangSC-Regular
}

.gopayContent .third .ul .li3 .right {
    color: #967800;
}


/* 第五层红包 */

.gopayContent .fifth {
    width: 86vw;
    background-color: #fff;
    padding: 0 7vw;
    padding-bottom: 1rpx;
}

.gopayContent .fifth .first {
    height: 7vh;
    overflow: hidden;
    margin: 0;
}

.gopayContent .fifth .left {
    float: left;
    width: 30%;
    line-height: 7vh;
    height: 7vh;
    font-size: 28rpx;
    font-family: PingFangSC-Regular;
}

.gopayContent .fifth .right {
    float: right;
    width: 60%;
    text-align: right;
    height: 7vh;
    overflow: hidden;
}

.gopayContent .fifth .right image {
    display: inline-block;
    width: 34rpx;
    height: 15rpx;
}

.gopayContent .fifth .right .span1 {
    display: inline-block;
    line-height: 7vh;
    height: 7vh;
    font-size: 28rpx;
    font-family: PingFangSC-Regular;
}

.gopayContent .fifth .second text {
    font-size: 28rpx;
    font-family: PingFangSC-Regular;
    line-height: 7vh;
    height: 7vh;
}

.gopayContent .fifth .second textarea {
    width: 94%;
    margin: 0 auto;
    height: 10vh;
    border: 1px solid #e5e5e5;
    color: #000;
    padding: 0 2vw;
    font-size: 25rpx;
    font-family: PingFangSC-Regular;
    padding: 15rpx;
}

.gopayContent .fifth .newBounsSpan {
    width: 30rpx;
    height: 30rpx;
    border: 1.5px solid #000;
    border-radius: 35rpx;
    display: block;
    float: right;
    margin-top: 25rpx;
    margin-right: 10rpx;
}

.gopayContent .fifth .newBounsSpan2 {
    float: right;
}

.gopayContent .fifth .newBounsSpan.active {
    background-color: #ffe600;
}


/* 红包列表 */

.gopayContent .hongbaolist {
    width: 100%;
    display: none;
}

.gopayContent .hongbaolist .li {
    padding: 3px 0;
    position: relative;
    height: 19vw;
}

.gopayContent .hongbaolist>.li>text {
    margin-left: 10vw;
    display: inline-block;
    vertical-align: 6vw;
    height: 5vw;
    width: 5vw;
    border: 1px solid #000;
    border-radius: 2.5vw;
    background-color: #fff;
}

.gopayContent .hongbaolist .li .hongbaolist_bottom {
    position: absolute;
    left: 32%;
    bottom: 1vw;
}

.gopayContent .hongbaolist .li .hongbaolist_bottom text {
    font-family: 'PingFangSC-Regular';
    display: block;
    text-align: center;
    font-size: 25rpx;
}

.gopayContent .hongbaolist .li .hongbaolist_bottom .name {
    font-size: 20rpx;
    color: #333;
}

.gopayContent .hongbaolist .li .hongbaolist_left {
    position: absolute;
    left: 0%;
    bottom: 0;
    height: 19vw;
    width: 19vw;
    text-align: center;
}

.gopayContent .hongbaolist .li .hongbaolist_left text {
    font-family: Aril;
    font-size: 30rpx;
    color: #AA821E;
    line-height: 19vw;
    font-weight: 700;
    letter-spacing: -5rpx;
}

.gopayContent .hongbaolist>li>span.active {
    background-color: #ffe600;
}


/* 底部结算 */

.shopcarFooter {
    position: fixed;
    bottom: 0px;
    width: 100%;
    border-top: 1px solid #c8c8c8;
    overflow: hidden;
    background-color: #fff;
    text-align: center;
    z-index: 10;
}

.shopcarFooter .right {
    width: 25%;
    float: right;
    text-align: center;
    background-color: #ffe900;
    line-height: 80rpx;
}

.shopcarFooter .right text {
    font-family: PingFangSC-Regular;
    font-size: 30rpx;
}

.shopcarFooter .center {
    display: inline-block;
    width: 74%;
    text-align: left;
}

.shopcarFooter .center .first {
    overflow: hidden;
    padding-left: 40rpx;
}

.shopcarFooter .center .first view {
    float: left;
}

.shopcarFooter .center .first .left1 {
    float: left;
}

.shopcarFooter .center .first .left2 {
    float: right;
    text-align: right;
    padding-right: 3%;
}

.shopcarFooter .center .first text {
    font-family: Arial;
    font-size: 38rpx;
    color: #aa821e;
    font-weight: 700;
    display: inline-block;
}

.shopcarFooter .center .first .span1 {
    font-family: PingFangSC-Regular;
    font-size: 28rpx;
    color: #000;
    font-weight: 100;
}

.shopcarFooter .center .first .span2 {
    font-family: PingFangSC-Regular;
    font-size: 25rpx;
    color: #000;
    font-weight: 100;
}


/*红包页面*/

.redhot {
    width: 650rpx;
    margin: 0 auto;
}

.redhot .redhotlist .li {
    width: 700rpx;
    height: 130rpx;
    margin-bottom: 20rpx;
    position: relative;
}

.redhot .redhotlist .li image {
    width: 450rpx;
    height: 130rpx;
}

.redhot .redhotlist .li .redhotlist_left {
    position: absolute;
    top: 0;
    left: 285rpx;
    height: 130rpx;
    width: 150rpx;
    text-align: center;
}

.redhot .redhotlist .li .redhotlist_left view {
    line-height: 130rpx;
    font-size: 50rpx;
    font-weight: 700;
    color: #AA821E;
    font-family: Aril;
}

.redhot .redhotlist .li .redhotlist_left view .sign {
    font-weight: normal;
    letter-spacing: -4rpx;
    font-size: 30rpx;
}

.redhot .redhotlist .li .redhotlist_bottom {
    left: 14%;
    top: 70rpx;
    position: absolute;
}

.redhot .redhotlist .li .redhotlist_bottom text {
    font-family: 'PingFangSC-Regular';
    display: block;
    text-align: center;
    font-size: 15rpx;
}

.redhot .redhotlist .li .redhotlist_bottom .name {
    font-size: 18rpx;
    color: #333;
}

.gopayContent .redhotlist .li .round {
    margin-left: 13vw;
    display: inline-block;
    vertical-align: 6vw;
    height: 32rpx;
    width: 32rpx;
    border: 1.5px solid #000;
    border-radius: 2.5vw;
    background-color: #fff;
}

.gopayContent .redhotlist .li .round.active {
    background-color: #ffe600;
}


/* 用户授权 */

.gopayContent .fffirst .usersetting {
    position: absolute;
    top: 40rpx;
    left: 225rpx;
    display: block;
    width: 300rpx;
    height: 85rpx;
    line-height: 85rpx;
    font-size: 25rpx;
    background-color: #ffe900;
}

#masterBoxX {
    position: fixed;
    bottom: 130rpx;
    width: 100%;
    overflow: hidden;
    background-color: #fff;
    z-index: 10;
}

#masterBoxPt {
    position: fixed;
    bottom: 80rpx;
    width: 100%;
    overflow: hidden;
    background-color: #fff;
    z-index: 10;
}