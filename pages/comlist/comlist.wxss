.goCoList {
    background-color: #ededed;
    padding-top: 20rpx;
    padding-bottom: 20rpx;
}

.goCoList .comlist {
    margin-bottom: 20rpx;
    padding: 3vw;
    background-color: #fff;
}

.goCoList .top {
    overflow: hidden;
    height: 20vw;
}

.goCoList .top .left {
    float: left;
    width: 35vw;
}

.goCoList .top .left .goTLFirst {
    overflow: hidden;
}

.goCoList .top .left .goTLFirst .left1 {
    float: left;
    width: 10vw;
    height: 10vw;
    border: 1px solid #ededed;
    border-radius: 5vw;
    overflow: hidden;
}

.goCoList .top .left .goTLFirst .left1 image {
    width: 10vw;
    height: 10vw;
}

.goCoList .top .left .goTLFirst .right1 {
    float: left;
    width: 21vw;
    height: 10vw;
    line-height: 10vw;
    padding-left: 2vw;
    font-family: PingFangSC-Regular;
    font-size: 25rpx;
    overflow: hidden;
}

.goCoList .top .left .goTLSecond {
    margin-top: 10rpx;
}

.goCoList .top .left .goTLSecond image {
    display: inline-block;
    font-family: PingFangSC-Regular;
    font-size: 1rem;
    color: #838383;
    height: 5vw;
    width: 5vw;
    margin-right: 2vw;
    letter-spacing: 0.05rem;
}

.goCoList .top .right {
    width: 58vw;
    height: 20vw;
    float: right;
}

.goCoList .top .right image {
    display: inline-block;
    width: 18vw;
    height: 18vw;
}

.goCoList .comment {
    min-height: 10vw;
    line-height: 40rpx;
    font-family: PingFangSC-Regular;
    font-size: 25rpx;
    margin: 0 0 4vw;
    overflow: hidden;
}